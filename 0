import datetime


def parseDate(input_str):
    formats = [
        '%d %B %Y',
        '%d %b %Y',
    ]
    for fmt in formats:
        try:
            return datetime.datetime.strptime(date_str, fmt)
        except:
            pass
    return None


date_str = input('What is your birth date? ')

# print ('Your birth date is', date_str)

parsed_date = parseDate(date_str);

if parsed_date is None:
    print('Invalid date!')
else:
    month = parsed_date.date.month
    day = parsed_date.date.day
    print('Your month is',month,'and your day is',day)

